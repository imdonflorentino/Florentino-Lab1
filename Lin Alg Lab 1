# for Reserves1 in Reserves1:
#     print(Reserves1)
Reserves1 = [{'Feebas', 'Magikarp', 'Unown'}]
print('Output:\n')
for Reserves1 in Reserves1 :
    print(Reserves1)
Output:

{'Feebas', 'Magikarp', 'Unown'}
d = "Unown"
o = "Magikarp"
n = "Feebas"
party = ['Charmander', 'Pidgey', 'SandShrew', 'Rattata', 'Abra', ]
party.sort()
print(f"{party} + {d}")
print(f"{party} + {o}")
print(f"{party} + {n}")
['Abra', 'Charmander', 'Pidgey', 'Rattata', 'SandShrew'] + Unown
['Abra', 'Charmander', 'Pidgey', 'Rattata', 'SandShrew'] + Magikarp
['Abra', 'Charmander', 'Pidgey', 'Rattata', 'SandShrew'] + Feebas
